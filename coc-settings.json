{
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "go.goplsPath": "/Users/Admin/go/bin/gopls",
  "languageserver": {
    "vue" : {
      "command": "hls",
      "filetypes": ["vue"]
